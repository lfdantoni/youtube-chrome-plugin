{
  "name": "Youtube chrome plugin",
  "description": "Build an Extension!",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["tabs", "activeTab", "scripting"],
  "host_permissions": [
    "https://youtube.com/",
    "https://www.youtube.com/"
  ]
}
